{
	"name": "raw data to full dataset",
	"properties": {
		"activities": [
			{
				"name": "Collect raw data to full set",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": {
								"value": "@{pipeline().parameters.currentTable}",
								"type": "Expression"
							},
							"wildcardFileName": {
								"value": "*.parquet",
								"type": "Expression"
							},
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "MergeFiles"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Raw_data",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Full_dataset",
						"type": "DatasetReference",
						"parameters": {
							"fileName": {
								"value": "@pipeline().parameters.currentTable",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"currentTable": {
				"type": "string"
			}
		},
		"variables": {
			"tableName": {
				"type": "String"
			}
		},
		"folder": {
			"name": "infor-ln/raw data pipelines"
		},
		"annotations": [],
		"lastPublishTime": "2021-09-30T07:27:46Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}